# Example environment variables for Sahadhyayi
# Copy this file to .env and fill in your actual values

# Frontend (Vite reads VITE_* at build time)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend / Node (server.js)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
GOODREADS_KEY=your_goodreads_key_here
GOODREADS_SECRET=your_goodreads_secret_here
GOODREADS_CALLBACK_URL=your_goodreads_callback_url_here

# Optional: dedicated Edge Function token if needed for STT proxying
SUPABASE_EDGE_STT_KEY=your_edge_stt_key_here
PORT=4000
GEMINI_API_KEY=your_gemini_api_key_here
